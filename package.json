{
	"name": "@pnpm/npm-conf",
	"version": "2.2.2",
	"description": "Get the npm config",
	"license": "MIT",
	"repository": "pnpm/npm-conf",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"__prepublishOnly": "node src/lib/make.js",
