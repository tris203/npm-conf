		"@babel/traverse": "^7.23.2",
		"@npmcli/config": "^8.0.1",
		"@pnpm/eslint-config": "^1.1.0",
		"@pnpm/tsconfig": "^2.0.0",
		"@types/node": "^14.0.14",
		"eslint": "^8.52.0",
		"jest": "^29.7.0",
		"npm": "^5.0.4",
		"rimraf": "^5.0.5",
		"typescript": "^5.2.2"
